# 论坛配置
# 请在此处填入您的论坛 session cookie
FORUM_SESSION_COOKIE="your_session_cookie_here"
# 论坛基础URL，一般无需修改
FORUM_BASE_URL="https://www.deepflood.com"

# AI 配置 (例如 new-api, one-api, etc.)
# 请在此处填入您的 AI 服务 API Key
AI_API_KEY="your_ai_api_key_here"
# 请在此处填入您的 AI 服务基础 URL
AI_BASE_URL="your_ai_base_url_here"
# 使用的 AI 模型
AI_MODEL="your_ai_model_here"

# 全局回复开关
REPLY_ENABLED=true


# ==================================================
# 通知服务配置 (Notification Service Settings)
# ==================================================

# Telegram 机器人 Token
# 从 @BotFather 获取
TG_BOT_TOKEN=

# Telegram 用户的 User ID
# 可以发送 /start 给 @userinfobot 获取
TG_USER_ID=